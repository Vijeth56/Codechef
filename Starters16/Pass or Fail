#include <bits/stdc++.h>
using namespace std;
bool sol(int n, int x, int p){
    return ((x*3)-(n-x)>=p);
}
int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n, x, p;
	    cin>>n>>x>>p;
	    if(sol(n, x, p))
	        cout<<"PASS"<<endl;
        else
            cout<<"FAIL"<<endl;
	}
	return 0;
}
